oken', token);\n    localStorage.setItem('@GoBaber:user', JSON.stringify(user));\n\n    setData({ token, user });\n  }, []);\n\n  const signOut = useCallback(() => {\n    localStorage.removeItem('@GoBaber:token');\n    localStorage.removeItem('@GoBaber:user');\n\n    setData({} as AuthState);\n  }, []);\n  return (\n    <AuthContext.Provider value={{ user: data?.user, signIn, signOut }}>\n      {children}\n    </AuthContext.Provider>\n  );\n};\n\nexport function useAuth(): AuthContextData {\n  const context = useContext(AuthContext);\n\n  if (!context) {\n    throw new Error('useAuth must be used within an AuthProvider');\n  }\n\n  return context;\n}\n","C:\\Users\\ronni\\Documents\\Dev\\Projetos\\aulas\\Gostack11\\Nível-3\\08-iniciando-front-end-web\\src\\hooks\\toast.tsx",["116"],"C:\\Users\\ronni\\Documents\\Dev\\Projetos\\aulas\\Gostack11\\Nível-3\\08-iniciando-front-end-web\\src\\hooks\\index.tsx",[],"C:\\Users\\ronni\\Documents\\Dev\\Projetos\\aulas\\Gostack11\\Nível-3\\08-iniciando-front-end-web\\src\\components\\ToastContainer\\Toast\\styles.ts",[],"C:\\Users\\ronni\\Documents\\Dev\\Projetos\\aulas\\Gostack11\\Nível-3\\08-iniciando-front-end-web\\src\\components\\ToastContainer\\styles.ts",[],{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","replacedBy":"122"},{"ruleId":"123","replacedBy":"124"},{"ruleId":"125","replacedBy":"126"},{"ruleId":"117","replacedBy":"127"},{"ruleId":"119","replacedBy":"128"},{"ruleId":"121","replacedBy":"129"},{"ruleId":"123","replacedBy":"130"},{"ruleId":"125","replacedBy":"131"},{"ruleId":"132","severity":1,"message":"133","line":17,"column":3,"nodeType":"134","messageId":"135","endLine":17,"endColumn":14},{"ruleId":"117","replacedBy":"136"},{"ruleId":"119","replacedBy":"137"},{"ruleId":"121","replacedBy":"138"},{"ruleId":"123","replacedBy":"139"},{"ruleId":"125","replacedBy":"140"},{"ruleId":"117","replacedBy":"141"},{"ruleId":"119","replacedBy":"142"},{"ruleId":"121","replacedBy":"143"},{"